.=	100
i=	MEM 0
j=	MEM 2
k=	MEM 0
n=	MEM 0
DATA=	MEM 42
	MEM 69
	MEM 96
	MEM 123
	MEM 1023
DATPTR=	MEM DATA
MAIN:	NOP
L00001:	MOV *DATPTR i
	ADD DATPTR #1 DATPTR
	MOV *DATPTR k
	ADD DATPTR #1 DATPTR
	CMP i #0
	JLE L00002
